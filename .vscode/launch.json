{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "vite userscript",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "node",
			"program": "vite-script.js",
			"args": [
				"${relativeFile}"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "vite userscript (watch)",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "node",
			"program": "vite-script.watch.js",
			"args": [
				"${relativeFile}"
			]
		}
	]
}
